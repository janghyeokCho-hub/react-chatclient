{
  "name": "eumtalk",
  "version": "1.12.4",
  "description": "Eumtalk Messenger Client",
  "author": "Covision Corp.",
  "main": "build/electron/main.js",
  "appId": "com.covision.eumtalk",
  "keywords": [],
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 10"
  ],
  "scripts": {
    "build": "webpack --target web --config ./webpack.config.prod",
    "make:macos": "webpack --mode production --target electron-renderer --config ./webpack.config.prod && webpack --mode production --arch darwin --config ./webpack.config.ele && electron-builder build --macos --config electron-builder.config.js",
    "make:x64": "webpack --mode production --target electron-renderer --config ./webpack.config.prod && webpack --mode production --arch x64 --config ./webpack.config.ele && electron-builder build --win --x64 --config electron-builder.config.js",
    "make:ia32": "webpack --mode production --target electron-renderer --config ./webpack.config.prod && webpack --mode production --arch ia32 --config ./webpack.config.ele && electron-builder build --win --ia32 --config electron-builder.config.js",
    "build-dev": "webpack --mode development --config ./webpack.config.dev",
    "build-ele": "webpack --config ./webpack.config.ele",
    "start": "webpack-dev-server --mode development --hot --config ./webpack.config.dev",
    "electron-dev": "electron ./dev-build/electron/main.js dev",
    "electron-prod": "electron ./build/electron/main.js",
    "electron-dev-all": "webpack --mode development --target electron-renderer --config ./webpack.config.dev && webpack --mode development --config ./webpack.config.ele && electron ./dev-build/electron/main.js dev",
    "build-prod-all": "webpack --mode production --target electron-renderer --config ./webpack.config.prod && webpack --mode production --config ./webpack.config.ele",
    "postinstall": "electron-builder install-app-deps",
    "browserslist": "browserslist"
  },
  "license": "ISC",
  "devDependencies": {
    "@loadable/component": "^5.12.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^2.9.0",
    "electron": "15.1.2",
    "electron-builder": "22.13.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-updater": "4.2.0",
    "immer": "^4.0.1",
    "jimp": "^0.16.1",
    "js-file-download": "^0.4.8",
    "mkdirp": "^0.5.1",
    "node-macaddress": "^0.2.4",
    "pg": "^7.4.1",
    "qs": "^6.9.0",
    "react": "^16.9.0",
    "react-contextmenu": "^2.13.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.3.0",
    "uuidv4": "^6.2.5",
    "vite": "^2.9.8",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.0"
  },
  "dependencies": {
    "@agney/react-loading": "^0.1.2",
    "@electron/remote": "^1.2.1",
    "@jjordy/swr-devtools": "^1.0.5",
    "@journeyapps/sqlcipher": "5.2.0",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@toast-ui/react-editor": "^3.1.0",
    "auto-launch": "^5.0.5",
    "clsx": "^1.1.1",
    "combine-image": "^1.0.3",
    "exif-js": "^2.3.0",
    "exif2css": "^1.3.0",
    "iconv-lite": "^0.6.2",
    "image-extensions": "^1.1.0",
    "ip": "^1.1.5",
    "jschardet": "^2.3.0",
    "jszip": "^3.7.1",
    "knex": "file:lib/knex-0.95.11.tgz",
    "lru-cache": "^6.0.0",
    "macaddress": "^0.5.1",
    "material-table": "^1.69.3",
    "mobile-detect": "^1.4.4",
    "preact": "^10.5.13",
    "react-color": "2.19.3",
    "react-helmet": "^6.1.0",
    "spectre.css": "^0.5.3",
    "styled-components": "^5.3.3",
    "swr": "^0.5.6",
    "turndown": "^7.1.1"
  }
}
